      <div class="panel panel-model">
        <div class="panel-heading">Name / Stats</div>
          <table class="table stats modelstats">
            <thead>
              <tr>
                <th>Character/Model Name</th>
                <th># of Models</th>
                <th>Type</th>
                <th>Origin</th>
                <th>Character Tile(s)</th>
                <th>Hero Sheet</th>
              </tr>
            </thead>
