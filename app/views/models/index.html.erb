              <tr>
                <td class="model"><%= link_to model.name, model_path(model) %></td>
                <td><%= model.count %></td>
                <td><%= model.role %></td>
                <td><%= model.origin %></td>
                <td><% if model.tiles.any? %> &#x2714; <% end %></td>
                <td><% if model.heroes.any? %> &#x2714; <% end %></td>
              </tr>
          <% end %>
        </table>
      </div></div>
      <div class="col-md-1 col-sm-1"></div>
    </div>
  </div>

 <script>
   $(document).ready(function() {
     $('a#show_byorigin').click(function(event){
       event.preventDefault();
       $('a.toggles').children().removeClass('active');
       $('a#show_byorigin').children().addClass('active');
       $('div#by_type').hide();
       $('div#by_stat').hide();
       $('div#by_origin').show();
     });

     $('a#show_bytype').click(function(event){
       event.preventDefault();
       $('a.toggles').children().removeClass('active');
       $('a#show_bytype').children().addClass('active');
       $('div#by_origin').hide();
       $('div#by_stat').hide();
       $('div#by_type').show();
     });

     $('a#show_bystat').click(function(event){
       event.preventDefault();
       $('a.toggles').children().removeClass('active');
       $('a#show_bystat').children().addClass('active');
       $('div#by_origin').hide();
       $('div#by_type').hide();
       $('div#by_stat').show();
     });

   });

 </script>
